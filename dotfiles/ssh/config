Host github.com
	User git
	AddKeysToAgent yes
	IdentityFile ~/.ssh/id_github
Host bitbucket.org
	User git
	AddKeysToAgent yes
	IdentityFile ~/.ssh/id_bitbucket
Host opi*
	User nathan
	AddKeysToAgent yes
	IdentityFile ~/.ssh/id_opi
Host nathandesktop nathanlaptop
	User nathan
	AddKeysToAgent yes
	IdentityFile ~/.ssh/id_nathan
